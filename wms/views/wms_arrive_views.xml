<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="wms_package_arrive_act_window" model="ir.actions.act_window">
        <field name="name">Arrive form</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">wms.change.package.status.multi.wizard</field>
        <field name="view_mode">form</field>
    </record>

    <menuitem id="wms_arrive_menu"
              action="wms_package_arrive_act_window"
              parent="wms_root_menu"
              name="Arrive" sequence="0"/>

    <!-- FORM -->
    <record id="wms_package_arrive_form" model="ir.ui.view">
        <field name="name">wms.package.arrive.form</field>
        <field name="model">wms.change.package.status.multi.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="new_package_status"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>
